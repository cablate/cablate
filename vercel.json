{
  "rewrites": [
    { "source": "/linktree", "destination": "/linktree.html" },
    { "source": "/projects/AnxiousStock/index", "destination": "/projects/AnxiousStock/index.html" },
    { "source": "/confirm-subscription", "destination": "/mail-confirm-subscription.html" },
    { "source": "/cabmeme", "destination": "/projects/CabMeme/index.html" },
    { "source": "/projects/CabMeme", "destination": "/projects/CabMeme/index.html" },
    { "source": "/recommend", "destination": "/recommend/index.html" },
    { "source": "/recommend/courses/miss_marketing_0to1", "destination": "/recommend/courses/miss_marketing_0to1/index.html" },
    { "source": "/recommend/courses/mky_danow_0to1", "destination": "/recommend/courses/mky_danow_0to1/index.html" },
    { "source": "/recommend/courses/brickverse_ai_beginner", "destination": "/recommend/courses/brickverse_ai_beginner/index.html" },
    { "source": "/recommend/courses/brickverse_ai_advanced", "destination": "/recommend/courses/brickverse_ai_advanced/index.html" }
  ],
  "headers": [
    {
      "source": "/projects/CabMeme/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=86400"
        }
      ]
    },
    {
      "source": "/projects/CabMeme/(.*).mp4",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    }
  ],
  "trailingSlash": false
}
